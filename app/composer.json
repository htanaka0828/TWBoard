        "slim/psr7": "^1.6",
        "slim/http": "^1.3",
        "illuminate/database": "^10.3",
        "illuminate/pagination": "^10.3",
        "awurth/slim-validation": "^5.0",
        "monolog/monolog": "^3.3",
        "slim/csrf": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "TWB\\": "src/"
        }
    }
}
